{% for group in groups %}
    <div class="filtered-element">
        <div class="about-header about-header_prev content cf">
            <img src="{{ vich_uploader_asset(group[0], 'imageFile') }}" alt="{{ group[0].name }}" class="about-header__img" width="272px" height="136px">
            <div class="style-list-wrapper">
                <h2 class="about-header__header">
                    <a href="{{ path('group_show', { 'slug': group[0].slug }) }}" class="about-header__group">{{ group[0].name }}</a>
                </h2>
                {{ render(controller('AppBundle:Frontend/Group:genres', { 'group' : group[0] } )) }}
            </div>
            <ul class="group-statistic cf">
                {% if app.user %}
                    {% include '@App/frontend/group/bookmark_group.html.twig' with { 'group': group[0], 'userGroups': userGroups } %}
                {% endif %}
                <li class="group-statistic__item">
                    <p id="fans" class="group-statistic__link group-statistic__link_fans"><span>{{ group['likes'] }}</span> like</p></li>
                <li class="group-statistic__item f-r">
                    <a href="{{ path('group_show', { 'slug': group[0].slug }) }}" class="group-statistic__link group-statistic__but">Seemore</a>
                </li>
            </ul>
        </div>
    </div>
{% endfor %}
